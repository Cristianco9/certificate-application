{
  "name": "fingerprint-auth-api",
  "version": "0.0.1",
  "description": "The API to authenticate the students",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "dev-dep": "npm install && npm i nodemon morgan eslint eslint-config-prettier eslint-plugin-prettier prettier --save-dev -D",
    "test": "echo \"Error: no test specified\" && exit 1",
    "migrations:generate": "sequelize-cli migration:generate --name",
    "migrations:run": "sequelize-cli db:migrate",
    "migrations:revert": "sequelize-cli db:migrate:undo",
    "migrations:delete": "sequelize-cli db:migrate:undo:all"
  },
  "keywords": [
    "Node.js",
    "Express.js",
    "sequelize",
    "Mysql"
  ],
  "author": "Cristianco9 <cristian_cortes_ortiz@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "@hapi/boom": "^10.0.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "sequelize": "^6.37.3",
    "sequelize-cli": "^6.6.2"
  }
}
